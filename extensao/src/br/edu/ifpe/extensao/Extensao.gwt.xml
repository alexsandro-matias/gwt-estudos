<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, 
	so that your app can take advantage of the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.10.0//EN"
  "http://gwtproject.org/doctype/2.10.0/gwt-module.dtd">
<!-- Estranho por que o módulo quando criado no Netbeans não vem com atributo 
	<module></module>> -->
<module rename-to='extensao'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<!-- Eles são os módulo herdados -->
	<inherits name='com.google.gwt.user.User' />
	<stylesheet src="folha-estilo.css" />


	<!-- Adicionando estilos específicos - estes serão herdados como o nome 
		sugere: -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard' /> -->

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<!-- <inherits name='com.google.gwt.user.theme.clean.Clean' /> -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard' /> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<inherits name='com.google.gwt.user.theme.dark.Dark' />

	<!-- Other module inherits -->

	<!-- Specify the app entry point class. -->
	<!-- Neste pacote é onde há a conversão de código Java em Javascript. Que 
		adicionando outro EntryPoint de outro arquivo. <entry-point class='br.edu.ifpe.extensao.client.PainelPopUp' 
		é o ponto de entrada do código. Algo similiar ao método main em Java. -->

	<!-- <entry-point class='br.edu.ifpe.extensao.client.Outro' /> -->
	<entry-point
		class='br.edu.ifpe.extensao.client.TabPanelTest' />
	<!-- <entry-point class='br.edu.ifpe.extensao.client.Extensao' /> -->



	<!-- Specify the paths for translatable code -->
	<source path='
		client ' />
	<source path='shared' />

	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe" />
</module>
